{
  "name": "caherrera/fullcsv",
  "type": "library",
  "keywords": ["csv","csv reader","csv writer"],
  "homepage": "https://github.com/caherrera/fullcsv",
  "description": "FullCSV is a simple Object Oriented CSV manipulation library for PHP 5.6+",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos Herrera",
      "email" : "carlos@itline.cl"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "psr-0": {
      "FullCsv": "src/FullCsv"
    }
  },
  "autoload-dev": {
    "psr-0": { "Test\\FullCsv": "tests/FullCsv" }
  },
  "require-dev": {
    "phpunit/phpunit": "5.*"
  }
}
